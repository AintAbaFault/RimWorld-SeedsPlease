  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedeggplant</defName>
    <label>extract eggplant seeds</label>
    <description>Extract seeds from eggplant.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Raweggplant</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Raweggplant</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seedeggplant>3</Seedeggplant>
