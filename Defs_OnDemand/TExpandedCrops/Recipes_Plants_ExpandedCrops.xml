      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedLettuce>4</SeedLettuce>
    </products>
  </RecipeDef>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedCarrot</defName>
