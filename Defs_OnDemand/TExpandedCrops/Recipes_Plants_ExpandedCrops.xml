<Defs>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedTomato</defName>
    <label>extract tomato seeds</label>
    <description>Extract seeds from Tomatoes.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawTomatoes</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawTomatoes</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedTomato>3</SeedTomato>
    </products>
  </RecipeDef>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedLettuce</defName>
